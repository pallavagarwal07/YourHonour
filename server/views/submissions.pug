doctype html
html
  head
    title Code Submissions
    link(rel='stylesheet', href='/css/bootstrap.min.css')
    link(rel='stylesheet', href='/css/custom.css')
    script(src='/js/jquery.min.js')
    script(src='/js/bootstrap.min.js')
    script(src='/js/submissions.js')
    script(src='/js/common.js')
    meta(name='viewport' content='width=device-width, initial-scale=1')
  body
    .container
      .row
        .col-xs-4
          h1= 'SUBMISSIONS'
        .col-xs-8.vertical-center
          div.btn-group(role="group")
            a.btn.btn-default.active(href='/submissions' role='button' style="outline: none") Submissions
            a.btn.btn-default(href='/questions' role='button') Questions
            a.btn.btn-default(href='/dashboard' role='button') Dashboard
            a.btn.btn-default(href='/logout' role='button') Logout
      br
      div.row
        div.col-xs-12= msg || ""
      div.row
        div.col-md-6.col-md-offset-3.list-group
          div.panel.panel-default
            div.panel-heading User Submissions
            table.table(id='table1')
              thead
                tr
                  th= '#'
                  th= 'ID'
                  th= 'Status'
                  th= 'Result'
              tbody(id='tableBody')
